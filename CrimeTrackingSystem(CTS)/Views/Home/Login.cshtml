
@{
    ViewBag.Title = "Login";
}
@*login*@
<div class="signin bg-white p-5 rounded">
    <h1>Login</h1>
    <div class="mb-3">
        <i class="fa-solid fa-user"></i><label class="form-label fw-bolder">Username</label>
        <input type="text" class="cc-box"/>
    </div>
    <div class="mb-3">
        <i class="fa-solid fa-key"></i><label class="form-label fw-bolder">Password</label>
        <input type="text" class="cc-box"/>
    </div>
    <div class="d-flex justify-content-start align-items-center">
        <button class="send p-2">login</button>
    </div>
    <a href="@Url.Action("Signup","Home")" class="text-dark fw-bolder">click here to create account...</a>
</div>


