@model IEnumerable<CrimeTrackingSystem_CTS_.Models.Signup>
@{
    ViewBag.AdminTitle = "Users";
    ViewBag.PageTitle = "Users list";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="p-2">
    <a class="btn btn-sm btn-outline-warning" href="@Url.Action("Index","Admin")"><i class="fa-solid fa-arrow-left"></i></a>
</div>
<div class="table-container">
    <table class="table table-light display responsive nowrap table-bordered text-black" width="100%" id="tableJsId">
        <thead>
            <tr>
                
                <th>
                    <label>ID</label>
                </th>
                <th>
                    <label>Photo</label>
                </th>
                <th>
                    <label>Username</label>
                </th>
                <th>
                    <label>Email</label>
                </th>
                <th>
                    <label>Gender</label>
                </th>
                <th>
                    <label>Pincode</label>
                </th>
                <th>
                    <label>Address</label>
                </th>
                <th>
                    <label>Marital Status</label>
                </th>
                <th>
                    <label>Living Status</label>
                </th>
                <th>
                    <label>Adhaar</label>
                </th>
                <th>
                    <label>Phone no.</label>
                </th>
                <th>
                    <label>User role</label>
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.RegId)
                </td>
                <td>
                    <a href="@Url.Content(item.Photo)" target="_blank">
                        <img src="@Url.Content(item.Photo)" width="100" alt="user profile picture" />
                    </a>
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Username)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Gender)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Pincode)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.Address)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Mstatus)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Lstatus)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Adhaar)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Phone)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Role)
                </td>
            </tr>
            }
        </tbody>
    </table>

</div>


